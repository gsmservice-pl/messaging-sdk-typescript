# @gsmservice-pl/messaging-sdk-typescript SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: GATEWAY_API_
#
# Security environment variables:
GATEWAY_API_BEARER=your_bearer_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
GATEWAY_API_DEBUG=true
